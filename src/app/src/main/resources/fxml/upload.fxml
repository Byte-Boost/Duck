<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="350.0" prefWidth="500.0" style="-fx-background-color: #e3e3e3;"
            xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="net.byteboost.duck.gui.UploadController">
   <Button layoutX="400.0" layoutY="90.0" mnemonicParsing="false" onAction="#fileSelect" text="Load File"/>
   <Button layoutX="395.0" layoutY="300.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="95.0" text="Next"/>
   <Label fx:id="selectedFileField" layoutX="30.0" layoutY="85.0" prefHeight="25.0" prefWidth="350.0"
          style="-fx-background-color: #fafafa; -fx-padding: 5;">
      <font>
         <Font name="System Bold Italic" size="16.0"/>
      </font>
   </Label>
</AnchorPane>
